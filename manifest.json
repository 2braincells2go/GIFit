{
    "name": "GIFYT",
    "description": "Make a GIF from a YouTube video",
    "version": "0.0.0",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["*://www.youtube.com/*"],
        "js": ["vendor/patch-worker.js", "vendor/omggif.js", "vendor/b64.js", "vendor/gifter.js", "content.js"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [],
    "browser_action": {
        "default_title": "GIFYT"
    },
    "manifest_version": 2
}
