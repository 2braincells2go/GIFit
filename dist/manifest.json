{
    "name": "GIFit!",
    "description": "Make GIFs from YouTube videos",
    "version": "1.0.0",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["*://www.youtube.com/*"],
        "js": [
            "vendor/patch-worker.js",
            "content.js"
        ],
        "run_at": "document_idle"
    }],
    "web_accessible_resources": [
        "gif.js.map"
    ],
    "manifest_version": 2
}
