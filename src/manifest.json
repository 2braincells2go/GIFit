{
    "name": "GIFit!",
    "description": "Make GIFs from YouTube videos",
    "version": "0.0.1",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["*://www.youtube.com/*"],
        "js": [
            "scripts/vendor/patch-worker.js",
            "content.js"
        ],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "scripts/vendor/gif.worker.js",
        "scripts/vendor/gif.worker.js.map",
        "scripts/gif.js.map"
    ],
    "manifest_version": 2
}
