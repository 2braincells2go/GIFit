{
    "name": "GIFYT",
    "description": "Make a GIF from a YouTube video",
    "version": "0.0.0",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["*://www.youtube.com/*"],
        "js": [
            "scripts/vendor/omggif.js",
            "scripts/vendor/b64.js",
            "scripts/vendor/gifter.js",
            "scripts/content.js"
        ],
        "run_at": "document_end"
    }],
    "manifest_version": 2
}
